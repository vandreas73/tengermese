@model IList<PollModel>
@if (Model.Count > 0)
{
    <div class="section-bg section-padding">
        <div class="home-page-polls section-width">
            <div class="section-head">
                <h2>@T("Polls.Title")</h2>
            </div>
            @foreach (var poll in Model)
            {
                @await Html.PartialAsync("_Poll", poll)
            }
        </div>
    </div>
}
